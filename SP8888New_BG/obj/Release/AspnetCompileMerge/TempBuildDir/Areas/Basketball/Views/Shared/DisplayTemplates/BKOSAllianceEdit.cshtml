@model List<Models.BKOSAlliance>
@{
    Models.BKOSAlliance olddata = Model[0];
    Models.BKOSAlliance newdata = Model[1];
}
<div class="edit">
    @Common.AppData.GetGameTypeName("BKOS")
    <br>
    <div>
        (聯盟名稱)：
        <span>@olddata.AllianceName</span> --->
        <span>@newdata.AllianceName</span>
    </div>
    <br>
    <div>
        (顯示名稱)：
        <span>@olddata.ShowName</span> --->
        <span>@newdata.ShowName</span>
    </div>
    <br>
    <div>
        (顯示狀態)：
        <span>@(olddata.Display == true ? "显示" : "隐藏")</span>--->
        <span>@(newdata.Display == true ? "显示" : "隐藏")</span>
    </div>
</div>
