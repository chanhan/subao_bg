@model List<Models.BKOSAlliance>
@{
    Models.BKOSAlliance newdata = Model[0];
}
<div>
    <b>
        @Common.AppData.GetGameTypeName("BKOS")
        <br>
        (聯盟)：
        <font color="red">@newdata.AllianceName</font>
        <br>
        (顯示名稱)：
        <font color="red">@newdata.ShowName</font>
        <br>
        (顯示狀態)：
        <font color="red">@(newdata.Display == true ? "显示" : "隐藏")</font>
    </b>
</div>
