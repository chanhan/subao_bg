@model List<Models.BasketballTeam>
@{
    Models.BasketballTeam newdata = Model[0];
}
<div>
    <b>
        @Common.AppData.GetGameTypeName(newdata.GameType)
        <br>
        (隊伍名稱)：
        <font color="red">@newdata.TeamName</font>
        <br>
        (顯示名稱)：
        <font color="red">@newdata.ShowName</font>
        <br>
        (來源名稱)：
        <font color="red">@newdata.WebName</font>
        <br>
        (所屬聯盟)：
        <font color="red">@newdata.AllianceName</font>
        <br>
        (勝)：
        <font color="red">@newdata.W</font>
        <br>
        (敗)：
        <font color="red">@newdata.L</font>
        <br>
        (和)：
        <font color="red">@newdata.T</font>
        <br>
        (來源)：
        <font color="red">@newdata.SourceID</font>
        <br>s
    </b>
</div>
