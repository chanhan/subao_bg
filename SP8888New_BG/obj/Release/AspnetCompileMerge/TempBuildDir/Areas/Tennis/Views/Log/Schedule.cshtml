@model Tuple<List<Models.ViewModel.Tennis>, List<Models.ViewModel.Tennis>, int>
@{
    Layout = null;
    string viewName = Model.Item3 == 1 ? "SchedulesAdd" : (Model.Item3 == 2 ? "SchedulesDelete" : "SchedulesEdit");
    List<Models.ViewModel.Tennis> tennis = new List<Models.ViewModel.Tennis>();
    int index = 0;
}
@if (Model.Item2.Any())
{
    foreach (var newitem in Model.Item2)
    {
        if (Model.Item1.Any())
        {
            var olditem = Model.Item1[index];
            tennis.Add(olditem);
        }
        index++;
        tennis.Add(newitem);
        @Html.DisplayFor(p => tennis, viewName)
        tennis.Clear();
    }
}
else
{
    if (Model.Item1.Any())
    {
        foreach (var newitem in Model.Item1)
        {
            tennis.Add(newitem);
            @Html.DisplayFor(p => tennis, viewName)
            tennis.Clear();
        }
    }
}
