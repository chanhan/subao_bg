@model Tuple<List<Models.ViewModel.BasketBall>, List<Models.ViewModel.BasketBall>, int>
@{
    Layout = null;
    string viewName = Model.Item3 == 1 ? "BKOSSchedulesAdd" : (Model.Item3 == 2 ? "BKOSSchedulesDelete" : "BKOSSchedulesEdit");
    List<Models.ViewModel.BasketBall> OSbasket = new List<Models.ViewModel.BasketBall>();
    int index = 0;
}
@if (Model.Item2.Any())
{
    foreach (var newitem in Model.Item2)
    {
        if (Model.Item1.Any())
        {
            var olditem = Model.Item1[index];
            OSbasket.Add(olditem);
        }
        index++;
        OSbasket.Add(newitem);
        @Html.DisplayFor(p => OSbasket, viewName)
        OSbasket.Clear();
    }
}
else
{
    if (Model.Item1.Any())
    {
        foreach (var newitem in Model.Item1)
        {
            OSbasket.Add(newitem);
            @Html.DisplayFor(p => OSbasket, viewName)
            OSbasket.Clear();
        }
    }
}