@model List<Models.TennisAlliance>
@{
    Models.TennisAlliance olddata = Model[0];
    Models.TennisAlliance newdata = Model[1];
}
<div class="edit">
    @Common.AppData.GetGameTypeName("TN")
    <br>
    <div>
        (聯盟)：
        <span>@olddata.AllianceName</span> --->
        <span>@newdata.AllianceName</span>
    </div>
    <div>
        (顯示名稱)：
        <span>@olddata.ShowName</span> --->
        <span>@newdata.ShowName</span>
    </div>
    <div>
        (顯示狀態)：
        <span>@(olddata.Display == true ? "显示" : "隐藏")</span> --->
        <span>@(newdata.Display == true ? "显示" : "隐藏")</span>
    </div>
</div>
