@model List<Models.TennisAlliance>
@{
    Models.TennisAlliance newdata = Model[0];
}
<div>
    <b>
        @Common.AppData.GetGameTypeName("TN")
        <br>
        (聯盟名稱)：
        <font color="red">@newdata.AllianceName</font>
        <br>
        (顯示名稱)：
        <font color="red">@newdata.ShowName</font>
        <br>
        (顯示狀態)：
        <font color="red">@(newdata.Display == true ? "显示" : "隐藏")</font>
    </b>
</div>