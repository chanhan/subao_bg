@model Tuple<List<Models.SetTypeVal>, List<Models.SetTypeVal>, int>
@{
    Layout = null;
    string viewName = Model.Item3 == 1 ? "SetTypeValAdd" : "SetTypeVallEdit";
    List<Models.SetTypeVal> settypeval = new List<Models.SetTypeVal>();
    int index = 0;
}
@if (Model.Item2.Any())
{
    foreach (var newitem in Model.Item2)
    {
        if (Model.Item1.Any())
        {
            var olditem = Model.Item1[index];
            settypeval.Add(olditem);
        }
        index++;
        settypeval.Add(newitem);
        @Html.DisplayFor(p => settypeval, viewName)
        settypeval.Clear();
    }
}
else
{
    if (Model.Item1.Any())
    {
        foreach (var newitem in Model.Item1)
        {
            settypeval.Add(newitem);
            @Html.DisplayFor(p => settypeval, viewName)
            settypeval.Clear();
        }
    }
}
